{
  "name": "dotnet8-mvc-auth-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:css": "tailwindcss -i ./wwwroot/css/site.css -o ./wwwroot/css/tailwind.css --watch",
    "build:css:once": "tailwindcss -i ./wwwroot/css/site.css -o ./wwwroot/css/tailwind.css",
    "build:css:prod": "tailwindcss -i ./wwwroot/css/site.css -o ./wwwroot/css/tailwind.css --minify",
    "build:js": "esbuild ./wwwroot/js/tailadmin/tailadmin.js --bundle --outfile=wwwroot/js/bundle.js --minify",
    "build": "npm run build:css:once && npm run build:js",
    "build:prod": "npm run build:css:prod && npm run build:js",
    "dev": "npm run build:css",
    "watch": "concurrently \"npm run build:css\" \"dotnet watch run\""
  },
